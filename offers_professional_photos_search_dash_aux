CREATE VIEW 	business_intelligence.offers_professional_photos_search_dash_aux AS
SELECT		search_id,
			searched_at,
			searched_at_week,
			country,
			city,
			offer_id,
			CASE
			WHEN account_management_key_account = 0
			THEN 'SML'
			ELSE 'KA'
			END AS account_management_key_account ,
			offer_type,
			score,
			group_position,
			current_page,
			CASE
			WHEN roles LIKE '%PHOTO%'
			THEN 1
			ELSE 0
			END AS professional_photo,
			
			CASE
			WHEN offer_clicked IS NULL
			THEN 0
			ELSE 1
			END AS offer_clicked,
			CASE
			WHEN booking_id IS NULL
			THEN 0
			ELSE 1
			END AS has_request,
			CASE
			WHEN accepted_at IS NULL
			THEN 0
			ELSE 1
			END AS has_booking_accepted,
			CASE
			WHEN paid_at IS NULL
			THEN 0
			ELSE 1
			END AS has_paid_booking,
			sum AS net_revenue
FROM		business_intelligence.offers_professional_photos_search
WHERE		DATEDIFF(day, searched_at, current_date) <= 60
GROUP BY	search_id,
			searched_at,
			searched_at_week,
			country,
			city,
			offer_id,
			CASE
			WHEN account_management_key_account = 0
			THEN 'SML'
			ELSE 'KA'
			END ,
			offer_type,
			score,
			group_position,
			current_page,
			CASE
			WHEN roles LIKE '%PHOTO%'
			THEN 1
			ELSE 0
			END ,
			
			CASE
			WHEN offer_clicked IS NULL
			THEN 0
			ELSE 1
			END ,
			CASE
			WHEN booking_id IS NULL
			THEN 0
			ELSE 1
			END ,
			CASE
			WHEN accepted_at IS NULL
			THEN 0
			ELSE 1
			END ,
			CASE
			WHEN paid_at IS NULL
			THEN 0
			ELSE 1
			END ,
			sum


