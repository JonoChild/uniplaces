CREATE VIEW business_intelligence.offers_professional_photos_search_dash AS
SELECT		
			searched_at,
			searched_at_week,
			country,
			city,
			professional_photo,
			COUNT(search_id),
			COUNT(offer_id),
			SUM(offer_clicked),
			SUM(has_request),
			SUM(has_booking_accepted),
			SUM(has_paid_booking),
			SUM(net_revenue)			
FROM		business_intelligence.offers_professional_photos_search_dash_aux
GROUP BY	searched_at,
			searched_at_week,
			country,
			city,
			professional_photo
