CREATE VIEW 	business_intelligence.search_interactions_base AS 
SELECT			business_intelligence.search_interactions_base_aux.search_id,
				business_intelligence.search_interactions_base_aux.guest_id,
				business_intelligence.search_interactions_base_aux.tracking_id,
				business_intelligence.device_by_tracking_info.device,
				business_intelligence.device_by_tracking_info.device_model,
				business_intelligence.search_interactions_base_aux.country,
				business_intelligence.search_interactions_base_aux.city,
				business_intelligence.search_interactions_base_aux.sort_by,
				business_intelligence.search_interactions_base_aux.searched_at,
				business_intelligence.search_interactions_base_aux.searched_at_year,
				business_intelligence.search_interactions_base_aux.searched_at_month,
				business_intelligence.search_interactions_base_aux.searched_at_week,
				business_intelligence.search_interactions_base_aux.experiment_id,
				business_intelligence.search_interactions_base_aux.free_text_search,
				business_intelligence.search_interactions_base_aux.move_in,
				business_intelligence.search_interactions_base_aux.move_out,
				business_intelligence.search_interactions_base_aux.budget_minimum,
				business_intelligence.search_interactions_base_aux.budget_maximum,
				business_intelligence.search_interactions_base_aux.rent_types,
				business_intelligence.search_interactions_base_aux.number_of_bedrooms,
				business_intelligence.search_interactions_base_aux.number_of_bathrooms,
				business_intelligence.search_interactions_base_aux.accommodation_types,
				business_intelligence.search_interactions_base_aux.features,
				business_intelligence.search_interactions_base_aux.suitable_for_erasmus_students,
				business_intelligence.search_interactions_base_aux.suitable_for_domestic_students,
				business_intelligence.search_interactions_base_aux.suitable_for_professional_placement,
				business_intelligence.search_interactions_base_aux.gender_restrictions,
				business_intelligence.search_interactions_base_aux.occupancy_restrictions,
				business_intelligence.search_interactions_base_aux.bills_included,
				business_intelligence.search_results.nbr_results,
				business_intelligence.search_clicks.nbr_offers_clicked,
				business_intelligence.search_bookings_info.nbr_requests,
				business_intelligence.search_bookings_info.nbr_paid_bookings,
				business_intelligence.search_bookings_info.sum as net_revenue_eur
FROM			business_intelligence.search_interactions_base_aux
LEFT JOIN		business_intelligence.device_by_tracking_info 
					ON business_intelligence.device_by_tracking_info.tracking_id = business_intelligence.search_interactions_base_aux.tracking_id
LEFT JOIN		business_intelligence.search_results
					ON business_intelligence.search_results.search_id = business_intelligence.search_interactions_base_aux.search_id
LEFT JOIN		business_intelligence.search_clicks
					ON business_intelligence.search_clicks.search_id =  business_intelligence.search_interactions_base_aux.search_id
LEFT JOIN		business_intelligence.search_bookings_info
					ON business_intelligence.search_bookings_info.search_id = business_intelligence.search_interactions_base_aux.search_id
GROUP BY 		business_intelligence.search_interactions_base_aux.search_id,
				business_intelligence.search_interactions_base_aux.guest_id,
				business_intelligence.search_interactions_base_aux.tracking_id,
				business_intelligence.device_by_tracking_info.device,
				business_intelligence.device_by_tracking_info.device_model,
				business_intelligence.search_interactions_base_aux.country,
				business_intelligence.search_interactions_base_aux.city,
				business_intelligence.search_interactions_base_aux.sort_by,
				business_intelligence.search_interactions_base_aux.searched_at,
				business_intelligence.search_interactions_base_aux.searched_at_year,
				business_intelligence.search_interactions_base_aux.searched_at_month,
				business_intelligence.search_interactions_base_aux.searched_at_week,
				business_intelligence.search_interactions_base_aux.experiment_id,
				business_intelligence.search_interactions_base_aux.free_text_search,
				business_intelligence.search_interactions_base_aux.move_in,
				business_intelligence.search_interactions_base_aux.move_out,
				business_intelligence.search_interactions_base_aux.budget_minimum,
				business_intelligence.search_interactions_base_aux.budget_maximum,
				business_intelligence.search_interactions_base_aux.rent_types,
				business_intelligence.search_interactions_base_aux.number_of_bedrooms,
				business_intelligence.search_interactions_base_aux.number_of_bathrooms,
				business_intelligence.search_interactions_base_aux.accommodation_types,
				business_intelligence.search_interactions_base_aux.features,
				business_intelligence.search_interactions_base_aux.suitable_for_erasmus_students,
				business_intelligence.search_interactions_base_aux.suitable_for_domestic_students,
				business_intelligence.search_interactions_base_aux.suitable_for_professional_placement,
				business_intelligence.search_interactions_base_aux.gender_restrictions,
				business_intelligence.search_interactions_base_aux.occupancy_restrictions,
				business_intelligence.search_interactions_base_aux.bills_included,
				business_intelligence.search_results.nbr_results,
				business_intelligence.search_clicks.nbr_offers_clicked,
				business_intelligence.search_bookings_info.nbr_requests,
				business_intelligence.search_bookings_info.nbr_paid_bookings,
				business_intelligence.search_bookings_info.sum
