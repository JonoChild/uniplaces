
CREATE VIEW business_intelligence.offers_professional_photos_search_aux AS 
SELECT		business_intelligence.search_results_professional_photos_aux.search_id,
			business_intelligence.search_results_professional_photos_aux.searched_at,
			business_intelligence.search_results_professional_photos_aux.offer_id,
			business_intelligence.search_results_professional_photos_aux.group_position,
			business_intelligence.search_results_professional_photos_aux.current_page,
			business_intelligence.search_results_professional_photos_aux.roles,
			business_intelligence.search_results_professional_photos_aux.account_management_key_account,
			business_intelligence.search_results_professional_photos_aux.offer_type,
			business_intelligence.search_results_professional_photos_aux.score,
			business_intelligence.offers_clicked_professinal_photo_aux.search_id as offer_clicked,
			business_intelligence.search_results_professional_photos_aux.tracking_id
FROM		business_intelligence.search_results_professional_photos_aux
LEFT JOIN	business_intelligence.offers_clicked_professinal_photo_aux
				ON business_intelligence.search_results_professional_photos_aux.search_id = business_intelligence.offers_clicked_professinal_photo_aux.search_id
				AND business_intelligence.search_results_professional_photos_aux.offer_id = business_intelligence.offers_clicked_professinal_photo_aux.offer_id
GROUP BY		business_intelligence.search_results_professional_photos_aux.search_id,
			business_intelligence.search_results_professional_photos_aux.searched_at,
			business_intelligence.search_results_professional_photos_aux.offer_id,
			business_intelligence.search_results_professional_photos_aux.group_position,
			business_intelligence.search_results_professional_photos_aux.current_page,
			business_intelligence.search_results_professional_photos_aux.roles,
			business_intelligence.search_results_professional_photos_aux.account_management_key_account,
			business_intelligence.search_results_professional_photos_aux.offer_type,
			business_intelligence.search_results_professional_photos_aux.score,
			business_intelligence.offers_clicked_professinal_photo_aux.search_id,
			business_intelligence.search_results_professional_photos_aux.tracking_id
