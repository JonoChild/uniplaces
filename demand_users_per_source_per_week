-- CREATE VIEW business_intelligence.demand_users_per_source_per_week AS
SELECT		EXTRACT(year from date) as year,
			week,
			source,
			top_line_grouping,
			cost_grouping,
			sum(users) as users
FROM		alooma.demand_users_per_sorce_per_week
LEFT JOIN	business_intelligence.source_classification
				ON business_intelligence.source_classification.upl_source =
				alooma.demand_users_per_sorce_per_week.source
GROUP BY	EXTRACT(year from date),
			week,
			source,
			top_line_grouping,
			cost_grouping;
			
